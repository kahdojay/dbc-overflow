<%= form_for :question, url: {action: "create"} do |f| %>
  <%= label :title, "Title: " %>
  <%= f.text_field :title %><br><br>
  <%= f.hidden_field :user_id, value: current_user.id %><br><br>

  <%= f.label :tags %>
  <%= f.text_field :question_tags, value: '' %><br><br>


  <%= f.text_area :body %><br><br>

  <%= f.submit "Submit Question" %>
<% end %>